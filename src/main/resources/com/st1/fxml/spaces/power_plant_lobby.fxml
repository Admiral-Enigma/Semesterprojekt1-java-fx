<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="610" prefWidth="1200" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.st1.ui.controllers.PowerPlantLobbyController">
    <children>
        <ImageView fitHeight="610" fitWidth="1200"  pickOnBounds="true">
            <image>
                <Image url="https://cdna.artstation.com/p/marketplace/presentation_assets/000/442/056/large/file.jpg?1594348859" />
            </image>
        </ImageView>
        <Label fx:id="title" layoutX="22.0" layoutY="14.0" text="Power plant lobby" textFill="WHITE">
         <font>
            <Font size="30.0" />
         </font>
         <effect>
            <DropShadow blurType="GAUSSIAN" height="3.18" radius="1.0275" spread="1.0" width="2.93" />
         </effect></Label>
        <Button styleClass="custombutton" layoutX="117.0" layoutY="88.0" mnemonicParsing="false" onAction="#goOutside" prefHeight="89.0" prefWidth="163.0" text="Gå udenfor" />
        <Button styleClass="custombutton" layoutX="963.0" layoutY="496.0" mnemonicParsing="false" onAction="#goTurbine" prefHeight="89.0" prefWidth="163.0" text="Gå til Turbinerummet" />
        <Button styleClass="custombutton" layoutX="963.0" layoutY="88.0" mnemonicParsing="false" onAction="#goBoiler" prefHeight="89.0" prefWidth="163.0" text="Gå til reaktorrummet" />
        <VBox fx:id="containmentItem" layoutX="331.0" layoutY="96.0" prefHeight="73.0" prefWidth="100.0" />
    </children>
</AnchorPane>
